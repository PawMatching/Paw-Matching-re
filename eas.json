{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "GOOGLE_SERVICES_FILE": "GOOGLE_SERVICES_INFO_PLIST"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "preview",
      "env": {
        "GOOGLE_SERVICES_FILE": "GOOGLE_SERVICES_INFO_PLIST"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "GOOGLE_SERVICES_FILE": "GOOGLE_SERVICES_INFO_PLIST"
      }
    }
  },
  "submit": {
    "production": {},
    "preview": {
      "ios": {
        "appleId": "kinoko.miki@gmail.com",
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID",
        "appleTeamId": "9LQ9ZNFQ9M"
      }
    }
  }
}
